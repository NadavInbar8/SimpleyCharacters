{
  "Settings": {
    "DataFolders": [
      "Data",
      "Data2"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "localhost",
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "API Health Check",
        "Uri": "/health"
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://0.0.0.0:8081",
        "Protocols": "Http1AndHttp2"
      }
    }
  }
}